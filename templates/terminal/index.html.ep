% layout 'wide';

<div id="top">
    <div id="tabs">
        <span id="newtab">+</span>
    </div>
</div>
<div id="window"></div>

<script>
  document.addEventListener('DOMContentLoaded', function(){
    document.getElementById('newtab').addEventListener('click', () => {
        Terminal.Init("Terminal", "<%= url_for('terminal websocket')->to_abs->scheme('ws') %>");
      });
      document.getElementById('newtab').click()
  });
    
</script>