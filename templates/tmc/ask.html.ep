% layout 'main';#main


<div
  ng-app="TMC-Ask"
  ng-controller="Controll as ctrl"
  class="relative"
>

  <div class="right">

  </div>

  <h2>Заявки на ТМЦ для объектов строительства</h2>
  
  <div ng-if="ctrl.ready">
  
    <object-my data-on-select-obj="ctrl.OnSelectObj(obj)"></object-my>
    <div ng-if="param['объект']">

      <tmc-ask-form data-param="param"></tmc-ask-form>
      <tmc-ask-table data-param="param"></tmc-ask-table>
    
    </div>

  </div>


</div>